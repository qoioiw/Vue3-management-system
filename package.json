    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.0",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.29",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.7",
    "prettier": "^3.0.3",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier": "^9.0.5",
